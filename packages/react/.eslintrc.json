{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "project": "./tsconfig.json"
  },
  "extends": [
    "eslint-config-standard-with-typescript"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": ["off"],
    "@typescript-eslint/no-unused-vars-experimental": ["error"]
  }
}
